{
  "manifest_version": 2,
  "name": "Force Squash for Bitbucket",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
     
        "https://bitbucket.org/*/*/pull-requests/*",
         "http://bitbucket.org/*/*/pull-requests/*"
      ],
      "js": [ "content.js" ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}